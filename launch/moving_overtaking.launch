<?xml version="1.0"?>
<launch>
  <arg name="robot1_model" default="$(env TURTLEBOT3_MODEL)" doc="Model type for robot 1 [burger, waffle, waffle_pi]" />
  <arg name="robot1_x" default="0.0" />
  <arg name="robot1_y" default="0.0" />
  <arg name="robot1_z" default="0.0" />

  <arg name="robot2_model" default="$(env TURTLEBOT3_MODEL)" doc="Model type for robot 2 [burger, waffle, waffle_pi]" />
  <arg name="robot2_x" default="4.0" />
  <arg name="robot2_y" default="0.0" />
  <arg name="robot2_z" default="0.0" />


  <param name="/use_sim_time" value="true" />


  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find overtaking_maneuver)/gazebo/robot_world.world" />
    <arg name="paused" value="false" />
    <arg name="use_sim_time" value="true" />
    <arg name="gui" value="true" />
    <arg name="headless" value="false" />
    <arg name="debug" value="false" />
  </include>


  <group ns="agent1">
    <param name="robot_description" 
           command="$(find xacro)/xacro --inorder $(find turtlebot3_description)/urdf/turtlebot3_$(arg robot1_model).urdf.xacro" />
    <node pkg="gazebo_ros" type="spawn_model" name="spawn_robot1" 
          args="-urdf -model robot1_turtlebot3_$(arg robot1_model) -x $(arg robot1_x) -y $(arg robot1_y) -z $(arg robot1_z) -param robot_description" />
  </group>


  <group ns="agent2">
    <param name="tf_prefix" value="agent2/" />
    <param name="robot_description" 
           command="$(find xacro)/xacro --inorder $(find overtaking_maneuver)/Hamster/turtlebot3_$(arg robot2_model).urdf.xacro" />
    <node pkg="gazebo_ros" type="spawn_model" name="spawn_robot2" 
          args="-urdf -model robot2_turtlebot3_$(arg robot2_model) -x $(arg robot2_x) -y $(arg robot2_y) -z $(arg robot2_z) -param robot_description" />
  </group>


  <node name="path_creator" pkg="overtaking_maneuver" type="path_creator.py">
    <param name="odom" 		value="agent1/odom" />
    <param name="use_sim_time" value="true" />
  </node> 

  <node name="control" pkg="overtaking_maneuver" type="control.py">
    <param name="path" 		value="path" />
    <param name="odom" 		value="agent1/odom" />
    <param name="cmd" 		value="agent1/cmd_vel" />
    <param name="use_sim_time" value="true" />
  </node>

  <node name="scan" pkg="overtaking_maneuver" type="scan.py">
    <param name="scan" 		value="agent1/scan" />
    <param name="odom" 		value="agent1/odom" />
    <param name="use_sim_time" value="true" />
  </node>

  <node name="dummy" pkg="overtaking_maneuver" type="dummy.py">
    <param name="cmd" 		value="agent2/cmd_vel" />
    <param name="use_sim_time" value="true" />
  </node>

  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find overtaking_maneuver)/rviz/Rviz.rviz" />
</launch>

<!-- roslaunch turtlebot3_gazebo turtlebot3_empty_world.launch-->
<!-- roslaunch turtlebot3_teleop turtlebot3_teleop_key.launch -->
